 <ul class="space-y-4">
  {{#each models.sketchs}}
      <li class="bg-gray-700 flex items-center">
        <div class="flex-1 p-4">
          <h2 class="text-xl font-semibold text-gray-100">{{this.name}}</h2>
          <p class="text-gray-300">{{this.description}}</p>
        </div>
        <i class="fa-solid fa-eye self-start p-4 cursor-pointer text-white hover:text-gray-300 active:text-teal-400 transform transition-transform duration-200 hover:scale-110 active:scale-90" 
          onclick="StartSketch('{{this.id}}')">
        </i>
      </li>
  {{/each}}      
</ul>