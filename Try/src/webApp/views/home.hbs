{{! <h2>Sketchs</h2> }}

{{#each models.sketchs}}
  <div class="card">
    <div class="card-header">
      <span class="header-title">{{this.name}}</span>
      <span class="header-icon">
          <i class="fa-solid fa-eye inactive" onclick="StartSketch('{{this.id}}')"></i>
          <i class="fa-solid fa-bolt inactive"></i>
      </span>
    </div>
    <div class="card-content">
      {{this.description}}
      {{!-- <div class="actions">
        <span><a class="action" href="?show={{this.id}}">Show</a></span>
        <span><a class="action" href="?autostart={{this.id}}">Auto Start</a></span>
      </div> --}}
    </div>
  </div>
{{/each}}